{
  "name": "github-actions",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "watch": "nodemon --exec ts-node src/index.ts",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "test": "node tests/basic.test.js",
    "test:build": "npm run build && npm run test:integration",
    "test:integration": "node tests/integration.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/varun0310t/Github-Actions.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/varun0310t/Github-Actions/issues"
  },
  "homepage": "https://github.com/varun0310t/Github-Actions#readme",
  "description": "Basic Express app with TypeScript",
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.4.5",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
